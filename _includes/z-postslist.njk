<ul class="postlinks">
  {% for year, posts in postslist | reverse | groupByYear %}
    <li class="list-style-type:none">{{year}}
      <ul>
      {%- for post in posts %}
          <li>
            <a href="{{ post.url | url }}">{{ post.data.title }}
            </a>          
          </li>
      {%- endfor %}
      </ul>
    </li>
  {% endfor %}
</ul>
